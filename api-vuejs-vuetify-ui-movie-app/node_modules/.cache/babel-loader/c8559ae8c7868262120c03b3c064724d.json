t vé ở đây, có thể gửi dữ liệu đặt vé đến backend, hiển thị thông báo, v.v.\r\n      console.log(\"Đã xác nhận đặt vé!\");\r\n      // Sau khi xác nhận, bạn có thể đóng dialog/modal bằng cách gán `ticketDialog` về `false`\r\n      this.ticketDialog = false;\r\n    },\r\n    async fetchMovie(movieId) {\r\n      const response = await this.$http.get(\r\n          \"/movie/\" + movieId + \"?append_to_response=credits,videos,images\"\r\n      );\r\n      this.movie = response.data;\r\n    },\r\n\r\n    openYouTubeModel() {\r\n      this.mediaURL = this.youtubeVideo();\r\n      this.isVideo = true;\r\n    },\r\n    openImageModel() {\r\n      this.isVideo = false;\r\n    },\r\n    youtubeVideo() {\r\n      if (!this.movie.videos) return;\r\n      return (\r\n          \"https://www.youtube.com/embed/\" + this.movie.videos.results[0].key\r\n      );\r\n    },\r\n  },\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n.iframe-container {\r\n  overflow: hidden;\r\n  padding-top: 56.25%;\r\n  position: relative;\r\n}\r\n\r\n.iframe-container iframe {\r\n  border: 0;\r\n  height: 100%;\r\n  left: 0;\r\n  position: absolute;\r\n  top: 0;\r\n  width: 100%;\r\n}\r\n</style>"],"mappings":"AAsIA,OAAAA,IAAA;AACA,OAAAC,MAAA;AAEA;EACAC,UAAA;IACAF,IAAA;IACAC;EACA;EACAE,KAAA;IACA;MACAC,KAAA;QACAC,OAAA;UACAC,IAAA;QACA;QACAC,MAAA;UACAC,SAAA;QACA;MACA;MACAC,OAAA;MACAC,QAAA;MACAC,MAAA;MACAC,YAAA;IACA;EACA;EACAC,QAAA;IAEA,KAAAC,UAAA,MAAAC,MAAA,CAAAC,MAAA,CAAAC,EAAA;EAEA;EACAC,KAAA;IACA;MACAC,QAAA;QACA,KAAAL,UAAA,MAAAC,MAAA,CAAAC,MAAA,CAAAC,EAAA;MACA;MACAG,SAAA;IACA;EACA;EACAC,QAAA;IACAC,WAAA;MACA,iDAAAlB,KAAA,CAAAmB,WAAA;IACA;EACA;EACAC,OAAA;IACAC,eAAA;MACA;MACAC,OAAA,CAAAC,GAAA;MACA;MACA,KAAAf,YAAA;IACA;IACA,MAAAE,WAAAc,OAAA;MACA,MAAAC,QAAA,cAAAC,KAAA,CAAAC,GAAA,CACA,YAAAH,OAAA,8CACA;MACA,KAAAxB,KAAA,GAAAyB,QAAA,CAAA1B,IAAA;IACA;IAEA6B,iBAAA;MACA,KAAAtB,QAAA,QAAAuB,YAAA;MACA,KAAAxB,OAAA;IACA;IACAyB,eAAA;MACA,KAAAzB,OAAA;IACA;IACAwB,aAAA;MACA,UAAA7B,KAAA,CAAA+B,MAAA;MACA,OACA,wCAAA/B,KAAA,CAAA+B,MAAA,